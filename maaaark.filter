ItemDisplay[key CLVL>79]: %CONTINUE%
ItemDisplay[key]: %GRAY%%NAME%{ver 4.1.7%NL%*Maaaark Filter}

// ------------------------------------------------------------------------------
// Gold
// ------------------------------------------------------------------------------

ItemDisplay[GOLD<100 CLVL<18]:
ItemDisplay[GOLD<1000 (CLVL>17 AND CLVL<80)]:
ItemDisplay[GOLD<5000 CLVL>79]:

// ------------------------------------------------------------------------------
// General Formatting
// ------------------------------------------------------------------------------
//Show me that item is etheral
ItemDisplay[ETH UNI]: %GRAY%[eth] %GOLD%%NAME%%CONTINUE%
ItemDisplay[ETH RARE]: %GRAY%[eth] %YELLOW%%NAME%%CONTINUE%
ItemDisplay[RARE]: %YELLOW%%NAME%%CONTINUE%
ItemDisplay[ETH MAG]: %GRAY%[eth] %BLUE%%NAME%%CONTINUE%
ItemDisplay[ETH NMAG]: %GRAY%[eth] %GRAY%%NAME%%CONTINUE%
ItemDisplay[ETH CRAFT]: %GRAY%[eth] %ORANGE%%NAME%%CONTINUE%

//Show me NMAG SOCK=0 items with white color AND NMAG SOCK>0 with gray
ItemDisplay[(ARMOR OR WEAPON) NMAG SOCK=0]: %WHITE%%NAME%%CONTINUE%
ItemDisplay[(ARMOR OR WEAPON) NMAG SOCK>0]: %GRAY%%NAME%%CONTINUE%

//Show me number of sockets of every socketed item
ItemDisplay[SOCK>0]: %NAME% [%SOCKETS%]%CONTINUE%

//Show iLVL of every mag rare
ItemDisplay[(MAG OR RARE) ILVL<88]: %NAME% [%ILVL%]%CONTINUE%
ItemDisplay[(MAG OR RARE) (ILVL>87 AND ILVL<92)]: %NAME% [%ILVL%]%CONTINUE%
ItemDisplay[(MAG OR RARE) (ILVL>91 AND ILVL<99)]: %NAME% %ORANGE%[%ILVL%]%CONTINUE%
ItemDisplay[(MAG OR RARE) ILVL>98]: %NAME% %PURPLE%[%ILVL%]%CONTINUE%


//Show me price of every MAG or RARE weapon/armor after ID
ItemDisplay[NMAG CLVL<24 PRICE>9999]: %NAME% %DARK_GREEN%[10k+]$%CONTINUE%
ItemDisplay[MAG CLVL<24 PRICE>9999]: %NAME% %DARK_GREEN%[10k+]$%CONTINUE%
ItemDisplay[RARE CLVL<24 PRICE>9999]: %NAME% %DARK_GREEN%[10k+]$%CONTINUE%

ItemDisplay[NMAG !RW CLVL>23 CLVL<65 PRICE>19999]: %NAME% %DARK_GREEN%[20k+]$%CONTINUE%
ItemDisplay[MAG CLVL>23 CLVL<65 PRICE>19999]: %NAME% %DARK_GREEN%[20k+]$%CONTINUE%
ItemDisplay[RARE CLVL>23 CLVL<65 PRICE>19999]: %NAME% %DARK_GREEN%[20k+]$%CONTINUE%

ItemDisplay[NMAG !RW CLVL>64 PRICE>34999]: %NAME% %DARK_GREEN%[35k]$%CONTINUE%
ItemDisplay[MAG CLVL>64 PRICE>34999]: %NAME% %DARK_GREEN%[35k]$%CONTINUE%
ItemDisplay[RARE CLVL>64 PRICE>34999]: %NAME% %DARK_GREEN%[35k]$%CONTINUE%

ItemDisplay[wss]: %PURPLE%WORLDSTAR%CONTINUE%

//Show me quantitiy of all stackables when over 1
ItemDisplay[!(ARMOR OR WEAPON OR tbk OR ibk) QTY>1 !(RUNE>0)]: %NAME% %TAN%QTY %QTY%%CONTINUE%



ItemDisplay[(RARE OR CRAFT) FOOLS]: %NAME% %PURPLE%FOOLS MOD%CONTINUE%
ItemDisplay[(RARE OR CRAFT) (ED>199 AND ED<250)]: %NAME% %PURPLE%ED >200%CONTINUE%
ItemDisplay[(RARE OR CRAFT) (ED>249 AND ED<300)]: %NAME% %PURPLE%ED >250%CONTINUE%
ItemDisplay[(RARE OR CRAFT) (ED>299 AND ED<400)]: %NAME% %PURPLE%ED >300%CONTINUE%
ItemDisplay[(RARE OR CRAFT) (ED>399 AND ED<500)]: %NAME% %PURPLE%ED >400%CONTINUE%
ItemDisplay[(RARE OR CRAFT) (ED>499)]: %NAME% %PURPLE%ED >500%CONTINUE%



